# Ansible Deployment Configuration
# Copy this file to .env and fill in your actual values

# Local machine sudo password
ANSIBLE_LOCAL_SUDO_PASSWORD=

# SSH Configuration
ANSIBLE_SSH_KEY=~/.ssh/id_ed25519
ANSIBLE_SSH_PASSPHRASE=

# Remote Host Configuration
ANSIBLE_REMOTE_IP=
ANSIBLE_REMOTE_PORT=22
ANSIBLE_REMOTE_USER=
ANSIBLE_REMOTE_SUDO_PASSWORD=

# Python interpreter path on remote host
ANSIBLE_PYTHON_INTERPRETER=/usr/bin/python3